  "dependencies": {
    "@kununu/kununu-icons": "1.2.0",
    "@kununu/kununu-theme-v2": "2.0.1",
    "classnames": "2.2.6",
    "debounce": "1.0.2",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "react-aria-modal": "2.12.2",
    "react-autosuggest": "9.3.4",
    "react-datepicker": "1.4.1",
    "react-scroll": "1.7.9"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "scripts": {
    "start": "start-storybook -s ./static -p 3000",
    "build": "build-storybook -s ./static -c .storybook -o docs",
    "babel": "cross-env NODE_ENV=dist babel ./src/components --out-dir ./dist/components --ignore 'src/components/**/story.js' && babel ./utils --out-dir ./dist/utils",
    "presass": "node-sass ./src/components --output ./dist/components --importer node_modules/node-sass-module-importer",
    "sass": "postcss dist/components/**/*.css --replace --use autoprefixer --no-map",
    "dist": "npm run clean && npm run babel && npm run sass",
    "eslint": "eslint ./src/components ./tests --max-warnings 0 --ext jsx --ext js",
    "eslint-fix": "eslint ./src/components ./tests --max-warnings 0 --ext jsx --ext js --fix",
    "prepublish": "npm run dist",
    "clean": "rimraf ./dist",
    "sasslint": "sass-lint --verbose",
    "lint": "npm run eslint && npm run sass-lint",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage --testResultsProcessor=\"jest-junit\""
  },
  "jest-junit": {
    "output": "./coverage/junit.xml"
  }
}
