{
  "name": "nukleus",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^utils(.*)$": "<rootDir>/utils$1"
    },
    "moduleDirectories": [
      "components",
      "node_modules"
    ]
  },
  "version": "2.0.0",
  "description": "Shared components repo for kununu projects",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kununu/nukleus.git"
  },
  "author": "kununu",
  "bugs": {
    "url": "https://github.com/kununu/nukleus/issues"
  },
  "homepage": "https://github.com/kununu/nukleus#readme",
  "license": "ISC",
  "files": [
    "dist/",
    "package.json",
    "components"
  ],
  "readme": "README.md",
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "6.22.2",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-rename-import": "2.0.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "bootstrap-sass": "3.3.7",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "enzyme-to-json": "1.4.5",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "4.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-sorting": "0.3.0",
    "file-loader": "0.10.0",
    "font-awesome": "4.7.0",
    "font-awesome-webpack": "0.0.4",
    "identity-obj-proxy": "3.0.0",
    "in-publish": "2.0.0",
    "jest": "18.0.0",
    "less": "2.7.1",
    "node-sass": "4.5.0",
    "node-sass-module-importer": "0.1.0",
    "postcss-loader": "1.2.2",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-dom": "15.4.1",
    "react-router": "3.0.2",
    "react-test-renderer": "15.4.1",
    "rimraf": "2.5.4",
    "sass-lint": "1.10.2",
    "sass-loader": "4.1.0",
    "sasslint-loader": "0.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "kununu-theme": "3.0.0",
    "moment": "2.17.1",
    "react-autosuggest": "8.0.0",
    "react-datepicker": "0.40.0",
    "react-scroll": "1.4.4"
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --port 3000 ./playground",
    "babel": "babel ./components --out-dir ./dist",
    "sass": "node-sass ./components --output ./dist --importer node_modules/node-sass-module-importer",
    "dist": "npm run build && npm run sass",
    "build": "cross-env NODE_ENV=dist npm run babel",
    "eslint": "eslint ./components ./playground ./tests --max-warnings 0 --ext jsx --ext js",
    "eslint-fix": "eslint ./components ./playground ./tests --max-warnings 0 --ext jsx --ext js --fix",
    "prepublish": "in-publish && npm run dist",
    "clean": "rimraf ./dist",
    "sasslint": "sass-lint --verbose",
    "lint": "npm run eslint && npm run sasslint",
    "test": "jest",
    "coverage": "jest --coverage"
  }
}
