    "@babel/preset-stage-0": "^7.0.0",
    "@babel/register": "7.0.0",
    "@storybook/addon-a11y": "4.0.8",
    "@storybook/addon-actions": "4.0.8",
    "@storybook/addon-backgrounds": "4.0.8",
    "@storybook/addon-centered": "4.0.8",
    "@storybook/addon-events": "4.0.8",
    "@storybook/addon-info": "4.0.8",
    "@storybook/addon-jest": "4.0.8",
    "@storybook/addon-knobs": "4.0.8",
    "@storybook/addon-links": "4.0.8",
    "@storybook/addon-notes": "4.0.8",
    "@storybook/addon-options": "4.0.8",
    "@storybook/addon-storyshots": "4.0.8",
    "@storybook/addon-storyshots-puppeteer": "4.0.8",
    "@storybook/addon-storysource": "4.0.8",
    "@storybook/addon-viewport": "4.0.8",
    "@storybook/addons": "4.0.8",
    "@storybook/components": "4.0.8",
    "@storybook/core-events": "4.0.8",
    "@storybook/node-logger": "4.0.8",
    "@storybook/react": "4.0.8",
    "autoprefixer": "9.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "8.0.2",
    "babel-jest": "22.4.3",
    "babel-plugin-transform-rename-import": "2.3.0",
    "bootstrap-sass": "3.3.7",
    "cross-env": "5.2.0",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.3",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.9.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.9.0",
    "eslint-plugin-sorting": "0.3.0",
    "file-loader": "1.1.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.3",
    "jest-junit": "5.1.0",
    "less": "3.0.2",
    "node-sass": "4.9.0",
    "node-sass-module-importer": "0.1.0",
    "postcss-cli": "5.0.1",
    "postcss-loader": "2.1.4",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-markdown": "^3.6.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.4.1",
    "rimraf": "2.6.2",
    "sass-lint": "1.12.1",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
